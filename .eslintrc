{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
  },
  "settings": {
    "react": {
      "pragma": "h"
    }
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
